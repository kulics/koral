let main() Int = {
  let mut list = [Int]List.new();
  list.push_back(10);
  list.push_back(20);
  
  let mut list2 = list.copy();
  
  // Modify copy
  list2.push_back(30);
  list2.set(0, 99);
  
  // Verify original list is unchanged
  if list.count() <> 2 then panic("Original list count changed");
  if list[0] <> 10 then panic("Original list[0] changed");
  if list[1] <> 20 then panic("Original list[1] changed");
  
  // Verify copy is changed
  if list2.count() <> 3 then panic("Copy list count wrong");
  if list2[0] <> 99 then panic("Copy list[0] wrong");
  if list2[1] <> 20 then panic("Copy list[1] wrong");
  if list2[2] <> 30 then panic("Copy list[2] wrong");
  
  print_int(list.count());
  print_int(list2.count());
  
  0
}
// EXPECT: 2
// EXPECT: 3
