type Num(x Int)

given Num {
    __equals(self ref, other Num ref) Bool = {
        self.x == other.x
    }
}

let [T Equatable] isEqual(a T, b T) Bool = a == b

let main() Int = {
    print_bool(isEqual(Num(1), Num(1)))
    print_bool(isEqual(Num(1), Num(2)))
