// EXPECT: add:
// EXPECT: 30
// EXPECT: block:
// EXPECT: 20
// EXPECT: inference:
// EXPECT: 30

let add(x Int, y Int) Int = x + y

let blockExample(a Int, b Int) Int = {
    let result Int = a * b
    yield result
}

let testInference() Int = {
    let a = 10
    let b = 20
    yield a + b
}

let main() Int = {
    println("add:")
    println(add(10, 20))
    
    println("block:")
    println(blockExample(4, 5))
    
    println("inference:")
    println(testInference())
    
    yield 0
}
