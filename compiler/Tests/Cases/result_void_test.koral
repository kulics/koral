// Test case for Result<Void, String> type
// EXPECT: Success!
// EXPECT: Something went wrong

public let do_something(success Bool) [Void, String]Result = {
    if success then {
        [Void, String]Result.Ok({})
    } else {
        [Void, String]Result.Error("Something went wrong")
    }
}

public let main() Void = {
    when do_something(true) is {
        .Ok(_) then print_line("Success!"),
        .Error(msg) then print_line(msg),
    }
    
    when do_something(false) is {
        .Ok(_) then print_line("Success!"),
        .Error(msg) then print_line(msg),
    }
}
