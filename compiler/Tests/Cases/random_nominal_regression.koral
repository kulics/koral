// EXPECT: random_nominal_ok

using std.random.*

let main() Int = {
    let rng = DefaultRandomSource.from_seed(42, 43, 44, 45)
    let r = [DefaultRandomSource]Random.new(rng)

    let _i Int = r.next()
    let _u UInt = r.next()
    let _f Float64 = r.next()
    let _d Int = Int.random(rng)

    println("random_nominal_ok")
    yield 0
}
