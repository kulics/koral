let main() Int = {
    // Int
    let i1 Int = 123
    let i2 Int = 123
    let i3 Int = 124
    println(i1.hash() == i2.hash())
    println(i1.hash() <> i3.hash())

    // Int8 (no unary minus; use block)
    let s1 Int8 = (Int8){ yield 0 - 7 }
    let s2 Int8 = (Int8){ yield 0 - 7 }
    let s3 Int8 = (Int8){ yield 0 - 6 }
    println(s1.hash() == s2.hash())
    println(s1.hash() <> s3.hash())

    // UInt8
    let u8a UInt8 = (UInt8)255
    let u8b UInt8 = (UInt8)255
    let u8c UInt8 = (UInt8)254
    println(u8a.hash() == u8b.hash())
    println(u8a.hash() <> u8c.hash())

    // UInt64
    let u64a UInt64 = (UInt64)4294967296// 2^32
    let u64b UInt64 = (UInt64)4294967296
    let u64c UInt64 = (UInt64)4294967297
    println(u64a.hash() == u64b.hash())
    println(u64a.hash() <> u64c.hash())

    // Float32
    let f32a Float32 = (Float32)1.5
    let f32b Float32 = (Float32)1.5
    let f32c Float32 = (Float32)2.5
    println(f32a.hash() == f32b.hash())
    println(f32a.hash() <> f32c.hash())

    // Float64
    let f64a Float64 = 1.5
    let f64b Float64 = 1.5
    let f64c Float64 = 2.5
    println(f64a.hash() == f64b.hash())
    println(f64a.hash() <> f64c.hash())

    // Pointer
    let p1 = [Int]alloc_memory(1)
    let p2 = [Int]alloc_memory(1)
    let p1b = p1
    println(p1.hash() == p1b.hash())
    println(p1.hash() <> p2.hash())
    [Int]dealloc_memory(p1)
    [Int]dealloc_memory(p2)

    yield 0
}
