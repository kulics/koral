// Test access control - entry file
using ..."./internal"
public using "./child"

public let main() Void = {
    // Test public symbol access
    let pub_val = get_public_value()
    check_assert(pub_val == 100, "public value should be 100")
    
    // Test protected symbol access (same module)
    let prot_val = get_protected_value()
    check_assert(prot_val == 200, "protected value should be 200")
    
    // Test child module public access
    let child_val = child.child_public_func()
    check_assert(child_val == 300, "child public value should be 300")
    
    print("All access control tests passed!")
}

let check_assert(condition Bool, message String) Void = {
    if not condition then {
        print("ASSERTION FAILED: ")
        print(message)
    }
}
