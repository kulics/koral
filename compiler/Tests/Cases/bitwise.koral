let main() Int = {
    printString("bitand:");
    printInt(3 bitand 1);
    
    printString("bitor:");
    printInt(1 bitor 2);
    
    printString("bitxor:");
    printInt(3 bitxor 1);
    
    printString("bitnot:");
    printInt(bitnot 0);
    
