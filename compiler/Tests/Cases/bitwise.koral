let main() Int = {
    print_line("bitand:")
    print_line(3 bitand 1)
    
    print_line("bitor:")
    print_line(1 bitor 2)
    
    print_line("bitxor:")
    print_line(3 bitxor 1)
    
    print_line("bitnot:")
    print_line(bitnot 0)
    
    print_line("bitshl:")
    print_line(1 bitshl 2)
    
    print_line("bitshr:")
    print_line(4 bitshr 1)
    
    print_line("precedence_add_shl:")
    print_line(1 bitshl 2 + 1)// 1 << 3 = 8
    
    print_line("precedence_shl_and:")
    print_line(2 bitand 1 bitshl 1)// 2 & (1 << 1) -> 2 & 2 -> 2
    
    print_line("precedence_and_or:")
    print_line(4 bitor 2 bitand 1)// 4 | (2 & 1) -> 4 | 0 -> 4
    
