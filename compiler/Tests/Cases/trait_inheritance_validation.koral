// Tests for inherited trait constraint validation
// EXPECT-ERROR: does not conform to trait Ordering

// Type with only Equality (not Ordering)
type OnlyEquatable(x Int)

given OnlyEquatable {
    equals(self ref, other OnlyEquatable ref) Bool = self.x == other.x
}

// Generic function requiring Ordering (which inherits from Equality)
let [T Ordering]find_max(a T, b T) T = if a > b then a else b

let main() Int = {
    let a = OnlyEquatable(1)
    let b = OnlyEquatable(2)
    // This should fail because OnlyEquatable doesn't implement Ordering
    let result = find_max(a, b)
    0
}
