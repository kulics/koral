let main() Int = {
    // Test Option
    let some_int = [Int]Option.Some(42)
    let none_int = [Int]Option.None()
    println(some_int)
    println(none_int)
    
    // Test List
    let mut list = [Int]List.new()
    list.push(1)
    list.push(2)
    list.push(3)
    println(list)
    
    // Test empty List
    let empty_list = [Int]List.new()
    println(empty_list)
    
    // Test empty Map
    let empty_map = [Int, String]Map.new()
    println(empty_map)
    
    // Test empty Set
    let empty_set = [Int]Set.new()
    println(empty_set)
    
    yield 0
}
// EXPECT: Some(42)
// EXPECT: None
// EXPECT: [1, 2, 3]
// EXPECT: []
// EXPECT: []
// EXPECT: []
