// EXPECT-ERROR: does not explicitly implement trait Beta

trait Alpha {
    keep(self) Self
}

trait Beta {
    keep(self) Self
}

type Box(v Int)

given Box Alpha {
    keep(self) Self = self
}

let [T Alpha]id(x T) T = x
let [T Beta]need_beta(x T) T = x

let main() Int = {
    let b = Box(1)
    let x = id(b)
    let _ = need_beta(x)
    yield 0
}
