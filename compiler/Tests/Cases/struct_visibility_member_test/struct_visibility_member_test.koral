// Test field visibility in regular member access
// Private field 'y' of Secret should not be accessible via .y from another file
// EXPECT-ERROR: Cannot access private field 'y' of type 'Secret'

using ..."./types"

public let main() Int = {
    let s = Secret(10, 20)
    // This should fail: trying to access private field via member access
    let val = s.y
    println(val)
    yield 0
}
