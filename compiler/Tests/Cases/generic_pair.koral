type [T, U] Pair(first T, second U);

let main() = {
    let p = [Int, String]Pair(1, "hello");
    printInt(p.first);
    printString(p.second);
    
    let p2 = [String, Int]Pair("world", 2);
    printString(p2.first);
    printInt(p2.second);
}
// EXPECT: 1
// EXPECT: hello
// EXPECT: world
// EXPECT: 2
