type [T Any, U Any] Pair(first T, second U);

let main() = {
    let p = [Int, String]Pair(1, "hello");
    print_int(p.first);
    print_string(p.second);
    
    let p2 = [String, Int]Pair("world", 2);
    print_string(p2.first);
    print_int(p2.second);
}
// EXPECT: 1
// EXPECT: hello
// EXPECT: world
// EXPECT: 2
