// Test basic Range type construction and contains method

let main() = {
    // Test ClosedRange: 1..5 (1 <= x <= 5)
    let r1 = 1..5
    assert(r1.contains(1), "ClosedRange should contain start")
    assert(r1.contains(3), "ClosedRange should contain middle")
    assert(r1.contains(5), "ClosedRange should contain end")
    assert(not r1.contains(0), "ClosedRange should not contain below start")
    assert(not r1.contains(6), "ClosedRange should not contain above end")
    
    // Test ClosedOpenRange: 1..<5 (1 <= x < 5)
    let r2 = 1..<5
    assert(r2.contains(1), "ClosedOpenRange should contain start")
    assert(r2.contains(4), "ClosedOpenRange should contain end-1")
    assert(not r2.contains(5), "ClosedOpenRange should not contain end")
    assert(not r2.contains(0), "ClosedOpenRange should not contain below start")
    
    // Test OpenClosedRange: 1<..5 (1 < x <= 5)
    let r3 = 1<..5
    assert(not r3.contains(1), "OpenClosedRange should not contain start")
    assert(r3.contains(2), "OpenClosedRange should contain start+1")
    assert(r3.contains(5), "OpenClosedRange should contain end")
    
    // Test OpenRange: 1<..<5 (1 < x < 5)
    let r4 = 1<..<5
    assert(not r4.contains(1), "OpenRange should not contain start")
    assert(r4.contains(2), "OpenRange should contain start+1")
    assert(r4.contains(4), "OpenRange should contain end-1")
    assert(not r4.contains(5), "OpenRange should not contain end")
    
    // Test FromRange: 5... (5 <= x <= max)
    let r5 = 5...
    assert(r5.contains(5), "FromRange should contain start")
    assert(r5.contains(100), "FromRange should contain large value")
    assert(not r5.contains(4), "FromRange should not contain below start")
    
    // Test FromOpenRange: 5<... (5 < x <= max)
    let r6 = 5<...
    assert(not r6.contains(5), "FromOpenRange should not contain start")
    assert(r6.contains(6), "FromOpenRange should contain start+1")
    
    // Test ToRange: ...5 (min <= x <= 5)
    let r7 = ...5
    assert(r7.contains(5), "ToRange should contain end")
    assert(r7.contains(0), "ToRange should contain zero")
    assert(r7.contains(0 - 100), "ToRange should contain negative")
    assert(not r7.contains(6), "ToRange should not contain above end")
    
    // Test ToOpenRange: ...<5 (min <= x < 5)
    let r8 = ...<5
    assert(r8.contains(4), "ToOpenRange should contain end-1")
    assert(not r8.contains(5), "ToOpenRange should not contain end")
    
    print_line("All range basic tests passed!")
}
