let id_u8(x UInt8) UInt8 = x

let main() Int = {
    let a UInt8 = 'A'
    let b UInt8 = "B"
    let nl UInt8 = "\n"

    println(a)
    println(b)
    println(nl)

    let c = id_u8('C')
    println(c)

    println(a == "A")
    println("B" == b)
    println(a <> b)
    println(a <> "A")

    when a is {
        "A" then println(1),
        _ then println(0),
    }

    when b is {
        "C" then println(0),
        "B" then println(1),
        _ then println(2),
    }

    let s = "hi"
    when s is {
        "hi" then println(3),
        _ then println(4),
    }

    yield 0
}
