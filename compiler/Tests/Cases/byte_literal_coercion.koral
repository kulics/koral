let id_u8(x UInt8) UInt8 = x

let main() Int = {
    let a UInt8 = 'A'
    let b UInt8 = "B"
    let nl UInt8 = "\n"

    print_line(a)
    print_line(b)
    print_line(nl)

    let c = id_u8('C')
    print_line(c)

    print_line(a == "A")
    print_line("B" == b)
    print_line(a <> b)
    print_line(a <> "A")

    when a is {
        "A" then print_line(1),
        _ then print_line(0),
    }

    when b is {
        "C" then print_line(0),
        "B" then print_line(1),
        _ then print_line(2),
    }

    let s = "hi"
    when s is {
        "hi" then print_line(3),
        _ then print_line(4),
    }

    0
}
