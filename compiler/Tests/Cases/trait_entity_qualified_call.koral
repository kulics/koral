// EXPECT: 17
// EXPECT: 42

trait Tag {
    value(self) Int
}

given Tag {
    plus_ten(self) Int = self.value() + 10
    kind(self) Int = 42
}

type Num(x Int)

given Num Tag {
    value(self) Int = self.x
}

let main() Int = {
    let n = Num(7)
    println(n.(Tag)plus_ten().to_string())
    println(Num.(Tag)kind(n).to_string())
    yield 0
}
