type [T] Box(mut val T);

given[T] [T]Box {
    get_val(self ref) T = self.val;
    
    set_val(self ref, v T) Void = {
        self.val = v;
    }
};

let main() Void = {
    let b = [Int]Box(10);
    print_int(b.get_val());
    b.set_val(20);
    print_int(b.get_val());
};
// EXPECT: 10
// EXPECT: 20
