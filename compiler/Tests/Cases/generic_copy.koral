type [T, U] Pair(first T, second U);

let [T] duplicate(x T) [T, T]Pair = {
    [T, T]Pair(x.copy(), x)
};

type Point(x Int, y Int);

given Point {
    copy(self Point ref) Point = Point(self.x, self.y);
};

let main() = {
    let p1 = duplicate(100);
    printInt(p1.first);
    printInt(p1.second);

    let pt = Point(10, 20);
    let p2 = duplicate(pt);
    printInt(p2.first.x);
    printInt(p2.second.y);
}
// EXPECT: 100
// EXPECT: 100
// EXPECT: 10
// EXPECT: 20
