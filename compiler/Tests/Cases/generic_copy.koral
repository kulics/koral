type [T, U] Pair(first T, second U);

let [T] duplicate(x T) [T, T]Pair = {
    [T, T]Pair(x, x)
};

type Point(x Int, y Int);

let main() = {
    let p1 = duplicate(100);
    printInt(p1.first);
    printInt(p1.second);

    let pt = Point(10, 20);
    let p2 = duplicate(pt);
    printInt(p2.first.x);
    printInt(p2.second.y);

    let p = ref 123;
    let p3 = duplicate(p);
    printInt(deref p3.first);
    printInt(deref p3.second);
}
// EXPECT: 100
// EXPECT: 100
// EXPECT: 10
// EXPECT: 20
// EXPECT: 123
// EXPECT: 123