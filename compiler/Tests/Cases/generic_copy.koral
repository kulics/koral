type [T Any, U Any] TestPair(first T, second U)

let [T Any] duplicate(x T) [T, T]TestPair = {
    [T, T]TestPair(x, x)
}

type Point(x Int, y Int)

let main() = {
    let p1 = duplicate(100)
    print_line(p1.first)
    print_line(p1.second)

    let pt = Point(10, 20)
    let p2 = duplicate(pt)
    print_line(p2.first.x)
    print_line(p2.second.y)

    let p = ref 123
    let p3 = duplicate(p)
    print_line(deref p3.first)
    print_line(deref p3.second)
}
