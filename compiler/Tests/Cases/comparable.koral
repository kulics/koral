let main() Int = {
    println("a" < "b")
    println("ab" < "aa")
    println("a" < "aa")
    println("b" >= "a")

    println(max("a", "b"))
    println(min("a", "b"))

    println(false < true)
    println(true > false)

    println(max(false, true))
    println(min(false, true))

    // clamp tests
    if clamp(5, 1, 10) == 5 and clamp(-3, 0, 10) == 0 and clamp(15, 0, 10) == 10 and clamp(0, 0, 10) == 0 and clamp(10, 0, 10) == 10 and clamp(3.5, 1.0, 5.0) == 3.5 and clamp(0.0, 1.0, 5.0) == 1.0 and clamp(9.9, 1.0, 5.0) == 5.0 then {
        println("clamp_ok")
    }

    yield 0
}
