// EXPECT: 500ms < 2s
// EXPECT: 500
// EXPECT: 2
// EXPECT: -1
// EXPECT: 0.25

let main() Int = {
    let d1 = Duration.from_milliseconds(500)
    let d2 = Duration.from_seconds(2)
    let d3 = Duration.from_minutes(1)

    let sum = d1 + d2
    let diff = d2 - d1
    let doubled = d1 * 2
    let half = d2 / 2
    let ratio = d1.ratio(d2)

    if d1 < d2 then {
        println("500ms < 2s")
    }

    println(d1.as_milliseconds().to_string())
    println(d2.as_seconds().to_string())
    println((-1).to_string())
    println(ratio)

    // Use results so they are not optimized away
    println(sum.to_string())
    println(diff.to_string())
    println(doubled.to_string())
    println(half.to_string())
    println(d3.to_string())

    yield 0
}
