// EXPECT: 500ms < 2s
// EXPECT: 500
// EXPECT: 2
// EXPECT: -1
// EXPECT: 0.25

let main() Int = {
    let d1 = Duration.from_millis(500)
    let d2 = Duration.from_secs(2)
    let d3 = Duration.from_mins(1)

    let sum = d1 + d2
    let diff = d2 - d1
    let doubled = d1 * 2
    let half = d2 / 2
    let ratio = d1.ratio(d2)

    if d1 < d2 then {
        print_line("500ms < 2s")
    }

    print_line(d1.as_millis().to_string())
    print_line(d2.as_seconds().to_string())
    print_line((-1).to_string())
    print_line(ratio)

    // Use results so they are not optimized away
    print_line(sum.to_string())
    print_line(diff.to_string())
    print_line(doubled.to_string())
    print_line(half.to_string())
    print_line(d3.to_string())

    0
}
