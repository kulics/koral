// Test: C keyword escaping
// This test verifies that C keywords can be used as identifiers in Koral
// EXPECT: int value: 42
// EXPECT: char value: 65
// EXPECT: void result: 100
// EXPECT: auto value: 200
// EXPECT: register value: 300
// EXPECT: struct field: 10
// EXPECT: function param: 50
// EXPECT: extern test: 999

// Struct with C keyword as field name
type KeywordStruct(int Int)

// Test using C keywords as variable names
// Note: We use C keywords that are NOT Koral keywords
let main() Int = {
    // C89 keywords as variable names (not Koral keywords)
    let int = 42
    let char = 65
    let void = 100
    let auto = 200
    let register = 300
    
    print("int value: ")
    print_line(int)
    
    print("char value: ")
    print_line(char)
    
    print("void result: ")
    print_line(void)
    
    print("auto value: ")
    print_line(auto)
    
    print("register value: ")
    print_line(register)
    
    // Test C keyword as struct field name
    let s = KeywordStruct(10)
    print("struct field: ")
    print_line(s.int)
    
    // Test C keyword as function parameter
    let result = use_keyword_param(50)
    print("function param: ")
    print_line(result)
    
    // Test extern (C keyword) as function name
    let ext = extern_func(999)
    print("extern test: ")
    print_line(ext)
    
    yield 0
}

// Function with C keyword as parameter name
let use_keyword_param(int Int) Int = {
    yield int
}

// Function named after C keyword
let extern_func(value Int) Int = {
    yield value
}
