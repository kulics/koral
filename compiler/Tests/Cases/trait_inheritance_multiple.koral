// EXPECT: A
// EXPECT: B
// EXPECT: C

trait A {
    fa(self) Void
}

trait B {
    fb(self) Void
}

trait C A and B {
    fc(self) Void
}

type S()

given S {
    fa(self) Void = print_string("A")
    fb(self) Void = print_string("B")
    fc(self) Void = print_string("C")
}

let [T C] call_all(x T) Void = {
    x.fa()
    x.fb()
    x.fc()
}

let main() Void = {
    let s = S()
    call_all(s)
}
