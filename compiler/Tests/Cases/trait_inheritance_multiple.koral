// EXPECT: A
// EXPECT: B
// EXPECT: C

trait A {
    fa(self) Void
}

trait B {
    fb(self) Void
}

trait C A and B {
    fc(self) Void
}

type S()

given S A {
    fa(self) Void = println("A")
}

given S B {
    fb(self) Void = println("B")
}

given S C {
    fc(self) Void = println("C")
}

let [T C] call_all(x T) Void = {
    x.fa()
    x.fb()
    x.fc()
}

let main() Void = {
    let s = S()
    call_all(s)
}
