// types.koral - Defines structs with private/protected fields

public type SecretPoint(
    public x Int,
    private y Int
)

given SecretPoint {
    public get_y(self) Int = self.y
}

public type ProtectedData(
    public name Int,
    protected value Int
)

given ProtectedData {
    public get_value(self) Int = self.value
}
