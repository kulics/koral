// file_a.koral - First file with private symbols
// These private symbols should NOT conflict with file_b's same-named symbols

// Private variable - same name as in file_b
private let secret_value Int = 111

// Private function - same name as in file_b
private let compute_secret() Int = 1000

// Private type - same name as in file_b
private type SecretData(value Int)

// Public wrapper type to expose private type's value
public type WrapperA(value Int)

// Public accessors to test the private symbols
public let get_private_var_a() Int = secret_value

public let call_private_func_a() Int = compute_secret()

public let create_private_type_a() WrapperA = {
    let secret = SecretData(10)
    WrapperA(secret.value)
}
