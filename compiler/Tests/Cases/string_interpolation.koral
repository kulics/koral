// EXPECT: Hello Koral!
// EXPECT: Count: 3
// EXPECT: Mixed Koral has 3 messages
// EXPECT: Sum 7
// EXPECT: Escaped \(not interpolation)
// EXPECT: Single Koral!
// EXPECT: Start Koral End

let main() Int = {
    let name String = "Koral"
    let count Int = 3

    print_line("Hello \(name)!")
    print_line("Count: \(count)")
    print_line("Mixed \(name) has \(count) messages")
    print_line("Sum \(1 + (2 * 3))")
    print_line("Escaped \\(not interpolation)")
    print_line('Single \(name)!')
    print_line("Start \(name) End")

    yield 0
}
