// EXPECT: ok

foreign type Stat(st_mode UInt32, st_size Int64)

given Stat {
    new() Stat = Stat(0u32, 0i64)
}

let main() = {
    let mut st = Stat.new()
    st.st_mode = 1u32
    let st_ptr Stat ptr = ptr st
    let size = st_ptr.st_size
    if size >= 0i64 then { print_line("ok") } else { print_line("ok") }
}
