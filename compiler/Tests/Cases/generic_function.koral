let [T Any] identity(x T) T = x

type [T Any, U Any] Pair(first T, second U)

let [T Any] makePair(a T, b T) [T, T]Pair = {
    [T, T]Pair(a, b)
}

let main() = {
    print_line(identity(42))
    print_line(identity("hello"))
    
    let p = makePair(10, 20)
    print_line(p.first)
    print_line(p.second)
}
// EXPECT: 42
// EXPECT: hello
// EXPECT: 10
// EXPECT: 20
