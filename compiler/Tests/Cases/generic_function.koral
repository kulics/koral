let [T] identity(x T) T = x;

type [T, U] Pair(first T, second U);

let [T] makePair(a T, b T) [T, T]Pair = {
    [T, T]Pair(a, b)
};

let main() = {
    print_int(identity(42));
    print_string(identity("hello"));
    
    let p = makePair(10, 20);
    print_int(p.first);
    print_int(p.second);
}
// EXPECT: 42
// EXPECT: hello
// EXPECT: 10
// EXPECT: 20
