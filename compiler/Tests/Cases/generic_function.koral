let [T] identity(x T) T = x;

let [T] makePair(a T, b T) [T, T]Pair = {
    [T, T]Pair(a, b)
};

type [T, U] Pair(first T, second U);

let main() = {
    printInt(identity(42));
    printString(identity("hello"));
    
    let p = makePair(10, 20);
    printInt(p.first);
    printInt(p.second);
}
// EXPECT: 42
// EXPECT: hello
// EXPECT: 10
// EXPECT: 20
