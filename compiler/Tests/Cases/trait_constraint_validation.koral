// Tests for trait constraint validation during monomorphization
// EXPECT-ERROR: does not conform to trait Equality

// Type without Equality implementation
type NoEquals(x Int)

// Generic function requiring Equality
let [T Equality]check_equal(a T, b T) Bool = a == b

let main() Int = {
    let a = NoEquals(1)
    let b = NoEquals(2)
    // This should fail because NoEquals doesn't implement Equality
    let result = check_equal(a, b)
    0
}
