// Tests for trait constraint validation during monomorphization
// EXPECT-ERROR: does not conform to trait Equatable

// Type without Equatable implementation
type NoEquals(x Int);

// Generic function requiring Equatable
let [T Equatable]check_equal(a T, b T) Bool = a == b;

let main() Int = {
    let a = NoEquals(1);
    let b = NoEquals(2);
    // This should fail because NoEquals doesn't implement Equatable
    let result = check_equal(a, b);
    0
}
