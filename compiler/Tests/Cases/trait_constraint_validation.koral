// Tests for trait constraint validation during monomorphization
// EXPECT-ERROR: does not explicitly implement trait Eq

// Type without Eq implementation
type NoEquals(x Int)

// Generic function requiring Eq
let [T Eq]check_equal(a T, b T) Bool = a == b

let main() Int = {
    let a = NoEquals(1)
    let b = NoEquals(2)
    // This should fail because NoEquals doesn't implement Eq
    let result = check_equal(a, b)
    yield 0
}
