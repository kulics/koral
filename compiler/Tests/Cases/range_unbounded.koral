// Test unbounded Range iterator functionality (uses T.max() and T.min())

let main() = {
    // Test FromRange with small iteration (5... but only iterate a few)
    let mut count = 0
    for i = 5... then {
        count = count + 1
        if count >= 5 then {
            break
        }
    }
    assert(count == 5, "FromRange should iterate at least 5 times")
    
    // Test FromOpenRange (5<... starts from 6)
    let mut first_val = 0
    for i = 5<... then {
        first_val = i
        break
    }
    assert(first_val == 6, "FromOpenRange should start from 6")
    
    // Test ToRange with small values (...5 from min to 5)
    // We can't iterate from min, so just test contains
    let r1 = ...5
    assert(r1.contains(5), "ToRange should contain 5")
    assert(r1.contains(0 - 1000), "ToRange should contain negative values")
    
    // Test ToOpenRange
    let r2 = ...<5
    assert(r2.contains(4), "ToOpenRange should contain 4")
    assert(not r2.contains(5), "ToOpenRange should not contain 5")
    
    // Test FullRange contains
    let r3 = [Int]Range.FullRange()
    assert(r3.contains(0), "FullRange should contain 0")
    assert(r3.contains(1000), "FullRange should contain 1000")
    assert(r3.contains(0 - 1000), "FullRange should contain -1000")
    
    print_line("All unbounded range tests passed!")
}
