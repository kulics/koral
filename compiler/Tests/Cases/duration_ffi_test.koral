foreign type Timespec(tv_sec Int64, tv_nsec Int64)

let test_ffi_struct() Bool = {
    let ts = Timespec(1i64, 500_000_000i64)
    assert(ts.tv_sec == 1i64)
    assert(ts.tv_nsec == 500_000_000i64)

    let p = ptr ts
    assert(p.tv_sec == 1i64)

    true
}

let main() Int = {
    assert(test_ffi_struct())
    0
}
