// EXPECT: add:
// EXPECT: 300
// EXPECT: sub:
// EXPECT: 50
// EXPECT: mul:
// EXPECT: 200
// EXPECT: div:
// EXPECT: 50
// EXPECT: mod:
// EXPECT: 1
// EXPECT: precedence:
// EXPECT: 14
// EXPECT: block:
// EXPECT: 20
// EXPECT: complex:
// EXPECT: 11

let main() Int = {
    let a = 100
    let b = 200
    
    println("add:")
    println(a + b)

    println("sub:")
    println(100 - 50)

    println("mul:")
    println(10 * 20)

    println("div:")
    println(100 / 2)

    println("mod:")
    println(10 % 3)

    println("precedence:")
    println(2 + 3 * 4)// 2 + 12 = 14

    println("block:")
    println({yield 2 + 3} * 4)// 5 * 4 = 20

    println("complex:")
    println(100 / 10 + 5 % 2 * 3 - 2)
    // 100 / 10 = 10
    // 5 % 2 = 1
    // 1 * 3 = 3
    // 10 + 3 - 2 = 11

    yield 0
}
