// Test that next_int_in with empty range triggers panic
// Validates: Requirements 4.9

// EXPECT: Panic: next_int_in: empty range

using std.random.*

let main() Void = {
    let rng = DefaultRandomSource.from_seed(1, 2, 3, 4)
    let r = [DefaultRandomSource]Random.new(rng)
    // 5..<5 is an empty range
    let v = r.next_int_in(5..<5)
    println(v)
}
