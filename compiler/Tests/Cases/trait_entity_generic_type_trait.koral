trait [T Any] MyEq {
    equals(self, other T) Bool
}

given [T Any] [T]MyEq {
    neq(self, other T) Bool = not self.equals(other)
}

type [T Eq] Box(val T)

given [T Eq] [T]Box {
    equals(self, other [T]Box) Bool = self.val == other.val
}

given [T Eq] [T]Box [[T]Box]MyEq {
}

let main() Int = {
    let a = [Int]Box(1)
    let b = [Int]Box(2)
    println(a.neq(b))
    println(a.neq(a))
    yield 0
}
